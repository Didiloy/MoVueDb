// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Netflix {
  id    String     @id
  type String  
  title  String
  director  String
  cast  String
  country  String
  date_added  String
  release_year  String
  duration  String
  listed_in  String
  description  String
}

model Disney {
  id    String    @id  @unique
  type String  
  title  String
  director  String
  cast  String
  country  String
  date_added  String
  release_year  String
  duration  String
  listed_in  String
  description  String
}

model Amazon {
  id    String    @id @unique
  type String  
  title  String
  director  String
  cast  String
  country  String
  date_added  String
  release_year  String
  duration  String
  listed_in  String
  description  String
}